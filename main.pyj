
new Vue({
    'el': '#app',
    'data': {
        'a': 4
    },
    'computed': {
        'b': def(): return this.a + 1
        ,
        'c': def(): return this.a * 2
    },
    'methods': {
        'increment': def(): this.a = this.a + 1
        ,
        'decrement': def(): this.a = this.a - 1
    }
})
