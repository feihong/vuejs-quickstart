-inherit boilerplate.plim

-block title
  Vue.js Quickstart

-block extrahead
  script src='https://cdnjs.cloudflare.com/ajax/libs/vue/1.0.26/vue.min.js'
  link rel='stylesheet' href='styles.styl'

-block extrascript
  / script src='main.js'
  script src='main.pyj'

-block body
  .container
    h1 = title()

    div
      button.decrement -
      button.increment +

    #app
      p a: {{ a }}
      p b: {{ b }}
      p c: {{ c }}
